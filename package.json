{
  "name": "app",
  "private": true,
  "type": "module",
  "dependencies": {
    "@hotwired/stimulus": "^3.2.2",
    "@hotwired/turbo-rails": "^8.0.3",
    "@rails/ujs": "^7.1.3-2",
    "esbuild": "^0.20.1",
    "sass": "^1.72.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "postcss-loader": "^8.1.1",
    "postcss-scss": "^4.0.9",
    "rollup": "^4.13.0",
    "rollup-plugin-postcss": "^4.0.2",
    "tailwindcss": "^3.4.1"
  },
  "scripts": {
    "dev": "rollup -c --watch",
    "build": "rollup -c",
    "build:css": "postcss --parser postcss-scss ./app/assets/stylesheets/application.scss -o ./app/assets/builds/application.css",
    "tailwindcss": "postcss ./input.css -o ./public/stylesheets/output.css --watch"
  }
}